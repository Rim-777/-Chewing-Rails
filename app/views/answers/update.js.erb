<% if @answer.errors.present? %>

<% @answer.errors.full_messages.each do |message| %>
$('#answer_errors').html('<%= j message %>');
<% end %>



<% else %>
var answerId = '<%= @answer.id %>';
$('#existed_answer_body_' + answerId).html("<%= j @answer.body %>");
toggleEditAnswer(answerId, 200);
<% end %>

